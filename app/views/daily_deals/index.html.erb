<h1>Listing daily_deals</h1>

<table>
  <tr>
    <th>Dealername</th>
    <th>Dealerid</th>
    <th>Dealertype</th>
    <th>Dealerfavorite?</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @daily_deals.each do |daily_deal| %>
  <tr>
    <td><%= daily_deal.DealerName %></td>
    <td><%= daily_deal.DealerID %></td>
    <td><%= daily_deal.DealerType %></td>
    <td><%= daily_deal.DealerFavorite? %></td>
    <td><%= link_to 'Show', daily_deal %></td>
    <td><%= link_to 'Edit', edit_daily_deal_path(daily_deal) %></td>
    <td><%= link_to 'Destroy', daily_deal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Daily deal', new_daily_deal_path %>
